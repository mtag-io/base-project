{
  "openAuth": true,
  "roles": [
    "ADMIN",
    "REGISTERED",
    "ANONYMOUS"
  ],
  "passwordRegexp": "/((?=.*\\d)|(?=.*\\W+))(?![.\\n])(?=.*[A-Z])(?=.*[a-z]).*$/;",
  "autoLogin": false,
  "jwtPayloadFields": ["userId", "email"],
  "apiKeyOverride": true,
  "userKey": "user",
  "profileKey": "profile",
  "accessTokenKey": "accessToken",
  "refreshTokenKey": "refreshToken",
  "profileKeys": [
    "fullName",
    "avatar",
    "lang"
  ],
  "anonymousUser": {
    "userId": "",
    "role": "ANONYMOUS",
    "email": "",
    "profile": {
      "fullName": "Anonymous User",
      "avatar": "assets/img/default-avatar.png",
      "lang": "default"
    }
  }
}